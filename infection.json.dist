{
    "timeout": 10,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "Coalesce": {
            "ignore": [
                "*::__construct"
            ]
        },
        "Throw_": {
            "ignore": [
                "*Validator::validate"
            ]
        }
    }
}
